{
  "consumer": {
    "name": "example-consumer-service"
  },
  "interactions": [
    {
      "description": "a request to create an order",
      "providerStates": [
        {
          "name": "I need to create an order"
        }
      ],
      "request": {
        "body": {
          "customerId": "some string",
          "status": "some string"
        },
        "headers": {
          "Accept": "application/json",
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.customerId": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.status": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {}
        },
        "method": "POST",
        "path": "/orders"
      },
      "response": {
        "body": {
          "customerId": "some string",
          "dateTimePlaced": "2023-08-15T13:21:09.996Z",
          "orderId": "e2490de5-5bd3-43d5-b7c4-526e33f71304",
          "status": "some string"
        },
        "generators": {
          "body": {
            "$.orderId": {
              "type": "Uuid"
            }
          }
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.customerId": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.dateTimePlaced": {
              "combine": "AND",
              "matchers": [
                {
                  "format": "yyyy-MM-dd'T'HH:mm:ss.SSSX",
                  "match": "datetime"
                }
              ]
            },
            "$.orderId": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
                }
              ]
            },
            "$.status": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.0.0"
    },
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "orders-service"
  }
}